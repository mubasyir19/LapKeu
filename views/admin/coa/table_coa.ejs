<table id="example2" class="table table-bordered table-hover table-coa">
  <thead>
    <tr>
      <th>No.</th>
      <th>Kode Akun</th>
      <th>Nama Akun</th>
      <th>Posisi</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <%# for(let i = 0; i < getAllCategory.length; i++) { %>
    <tr>
      <td><%#= i + 1 %></td>
      <td><%#= getAllCategory[i].name %></td>
      <td><%#= getAllCategory[i].name %></td>
      <td><%#= getAllCategory[i].name %></td>
      <td class="d-flex">
        <a href="/article/category/edit/<%#= getAllCategory[i].id %>" class="btn btn-info btn-sm">
          <i class="fas fa-pencil-alt"></i>
          Ubah
        </a>
        <a
          href="javascript:void()"
          type="button"
          class="btn btn-danger btn-sm ml-1 button-delete-coa"
          data-id="<%#= getAllCategory[i].id %>"
        >
          <i class="fas fa-trash-alt"></i>
          Hapus
        </a>
      </td>
    </tr>
    <%# } %>
  </tbody>
</table>
