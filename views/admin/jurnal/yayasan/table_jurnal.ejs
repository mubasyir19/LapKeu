<table id="example2" class="table table-bordered table-hover table-jurnal">
  <thead>
    <tr>
      <!-- <th>No.</th> -->
      <th>Tanggal</th>
      <th>Keterangan</th>
      <th>REF</th>
      <th>Debit</th>
      <th>Kredit</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% for(let i = 0; i < jurnal.length; i++) { %>
    <tr>
      <!-- <td><%#= i + 1 %></td> -->
      <td>
        <%= jurnal[i].date.toLocaleDateString('id-ID', { weekday: 'long', day: 'numeric', month: 'long', year: 'numeric'
        }) %>
      </td>
      <td><%= jurnal[i].description %></td>
      <td>
        <span class="badge badge-success"><%= jurnal[i]?.coa?.code ?? '-' %></span>
      </td>
      <td>Rp.<%= jurnal[i].debit.toLocaleString() ?? '-' %></td>
      <td>Rp.<%= jurnal[i].kredit.toLocaleString() ?? '-' %></td>

      <td class="d-flex">
        <a href="#<%= jurnal[i].id %>" class="btn btn-info btn-sm">
          <i class="fas fa-pencil-alt"></i>
          Ubah
        </a>
        <a
          href="javascript:void()"
          type="button"
          class="btn btn-danger btn-sm ml-1 button-delete-catatan"
          data-id="<%= jurnal[i].id %>"
        >
          <i class="fas fa-trash-alt"></i>
          Hapus
        </a>
      </td>
    </tr>
    <% } %>
  </tbody>
</table>
